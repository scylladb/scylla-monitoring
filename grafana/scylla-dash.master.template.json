            {
                "class": "logo_row",
                "panels": [
                    {
                        "class": "text_panel",
                        "content": "<img src=\"http://www.scylladb.com/wp-content/uploads/logo-scylla-white-simple.png\" height=\"70\">\n<hr style=\"border-top: 3px solid #5780c1;\">"
                    }

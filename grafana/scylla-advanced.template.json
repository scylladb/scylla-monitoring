{
    "dashboard": {
        "class": "dashboard",
        "uid": "advanced-__SCYLLA_VERSION_DASHED__",
        "rows": [
            {
                "class": "small_stat_rows"
            },
            {
                "class": "row",
                "panels": [
                    {
                        "class": "collapsible_row_panel",
                        "title": ""
